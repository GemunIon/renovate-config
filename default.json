{
  "extends": [
    "config:base",
    ":disableRateLimiting",
    ":semanticCommits",
    "group:allNonMajor"
  ],
  "timezone": "Asia/Singapore",
  "schedule": [
    "before 5am on Saturday"
  ],
  "hostRules": [
    {
      "matchHost": "https://npm.pkg.github.com/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "npmrc": "@gemunion:registry=https://npm.pkg.github.com/",
  "packageRules": [
    {
      "matchPackageNames": ["umzug"],
      "allowedVersions": "<=3.0.0-beta.16"
    },
    {
      "matchPackageNames": ["notistack"],
      "allowedVersions": "<=1.0.6"
    },
    {
      "matchPackageNames": ["amqp-connection-manager"],
      "allowedVersions": "<=3.3.0"
    },
    {
      "matchPackagePatterns": ["^@gemunion/"],
      "rollbackPrs": false
    },
    {
      "updateTypes": [
        "major",
        "minor",
        "patch"
      ],
      "automerge": false
    },
    {
      "updateTypes": [
        "major"
      ],
      "masterIssueApproval": true
    },
    {
      "matchDepTypes": ["devDependencies"],
      "addLabels": ["dev"]
    },
    {
      "matchDepTypes": ["dependencies"],
      "addLabels": ["deps"]
    }
  ],
  "major": {
    "masterIssueApproval": true
  },
  "renovateFork": true,
  "dependencyDashboard": true
}
